{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/chat.js",
        "dest": "/public/chat.js"
      },
      {
        "src": "/styles.css",
        "dest": "/public/styles.css"
      },
      {
        "src": "/orb.png",
        "dest": "/public/orb.png"
      },
      {
        "src": "/(.*)",
        "dest": "/server.js"
      }
    ]
  }
  